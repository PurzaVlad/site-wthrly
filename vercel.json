{
    "version": 2,
    "builds": [
      {
        "src": "wthrly/manage.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/staticfiles/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/wthrlyapp/templates/wthrlyapp/$1"
      }
    ],
    "buildCommand": "python manage.py collectstatic --noinput"
  }