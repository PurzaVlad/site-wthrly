{
  "functions": {
    "api/**/*.py": {
      "runtime": "python3.9"
    }
  },
  "builds": [
    {
      "src": "wthrlyapp/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/wthrlyapp/wsgi.py"
    }
  ]
}
